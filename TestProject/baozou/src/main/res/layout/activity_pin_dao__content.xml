<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qf.administrator.baozou.activity.PinDao_ContentActivity">
    <WebView
        android:id="@+id/pindao_webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></WebView>
    <RelativeLayout
        android:id="@+id/bar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/backgroud_red"
        android:gravity="center_vertical">
        <ImageView
            android:id="@+id/img_1"
            android:src="@drawable/yyb_appdetail_showmore"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"/>
        <ImageView
            android:id="@+id/mulu"
            android:src="@mipmap/ic_launcher"
            android:onClick="back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:id="@+id/img_2"
            android:src="@mipmap/abc_btn_rating_star_off_mtrl_alpha"
            android:layout_toLeftOf="@id/img_1"
            android:layout_centerVertical="true"
            android:layout_width="30dp"
            android:layout_height="30dp" />
        <TextView
            android:id="@+id/comment_Num"
            android:text="123"
            android:textColor="#fff"
            android:textSize="18sp"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@id/img_2"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:id="@+id/img_3"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@id/comment_Num"
            android:layout_centerVertical="true"
            android:src="@drawable/btn_actionbar_comment_nor"
            android:layout_width="25dp"
            android:layout_height="25dp" />
        <TextView
            android:id="@+id/like_Num"
            android:text="321"
            android:textColor="#fff"
            android:textSize="18sp"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@id/img_3"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_toLeftOf="@id/like_Num"
            android:layout_centerVertical="true"
            android:layout_marginRight="5dp"
            android:src="@drawable/btn_actionbar_like_nor"
            android:layout_width="25dp"
            android:layout_height="25dp" />
    </RelativeLayout>
</RelativeLayout>
